{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "indent": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "eol-last": "off",
    "max-len": "off",
    "space-before-function-paren": "off",
    "no-unused-vars": "off",
    "prefer-const": "off",
    "comma-spacing": "off",
    "no-prototype-builtins": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}